    {{ form_start(form) }}
        <div>{{ form_row(form.name) }}</div>
        <div>{{ form_row(form.description) }}</div>
        <div>{{ form_row(form.picture) }}</div>
        <div>{{ form_row(form.parent) }}</div>  
        <div>{{ form_row(form.color) }}</div>
        <div>{{ form_row(form.isActived) }}</div>
        <div>{{ form_row(form.isDeleted) }}</div>    
        <button class="btn btn-primary">{{ button|default('form_btn_valider'|trans)}}</button>
    {{ form_end(form) }}